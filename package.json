{
  "name": "@mizdra/eslint-interactive",
  "description": "The CLI tool to run `eslint --fix` for each rule",
  "version": "0.0.0",
  "repository": "https://github.com/mizdra/eslint-interactive.git",
  "author": "mizdra <pp.mizdra@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "tsc -p tsconfig.src.json",
    "dev": "tsc-watch -p tsconfig.src.json --onSuccess 'bin/eslint-interactive fixtures'",
    "check": "run-s -c check:*",
    "check:tsc": "run-s -c check:tsc:*",
    "check:tsc:src": "tsc -p tsconfig.src.json --noEmit",
    "check:tsc:test": "tsc -p tsconfig.test.json --noEmit",
    "check:eslint": "eslint src test",
    "test": "jest --colors"
  },
  "devDependencies": {
    "@jest/types": "^26.6.2",
    "@mizdra/eslint-config-mizdra": "^0.7.0",
    "@mizdra/prettier-config-mizdra": "^0.3.0",
    "@types/cli-table": "^0.3.0",
    "@types/eslint": "^7.2.6",
    "@types/node": "^14.14.10",
    "@types/terminal-link": "^1.2.0",
    "@types/yargs": "^15.0.12",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.0",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "2.2.1",
    "ts-jest": "^26.4.4",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "cli-table": "^0.3.4",
    "enquirer": "^2.3.6",
    "node-pager": "^0.3.4",
    "ora": "^5.1.0",
    "source-map-support": "^0.5.19",
    "terminal-link": "^2.1.1",
    "yargs": "^16.2.0"
  },
  "peerDependencies": {
    "eslint": ">=3.0"
  },
  "engines": {
    "node": ">=12.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "bin": {
    "eslint-interactive": "bin/eslint-interactive"
  },
  "keywords": [
    "eslint"
  ],
  "files": [
    "src",
    "dist"
  ]
}
